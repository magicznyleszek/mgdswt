{% spaceless %}

    {% include "partials/html-head.twig" %}

    {% block base_header %}
        {% include "partials/header.twig" %}
    {% endblock %}

    <div id="appContentWrapper">

        <section id="appContent" role="main" am-box="paddedH" {% block app_content_attributes %}{% endblock %}>

            {% block content %}{% endblock %}

        </section>

        {% block sidebar %}
            {% include "partials/sidebar.twig" %}
        {% endblock %}

    </div>

    {% block base_footer %}
        {% include "partials/footer.twig" %}
    {% endblock %}

    {% include "partials/html-end.twig" %}

{% endspaceless %}
